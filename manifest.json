{
	"manifest_version" : 2,
	"name":"Link Save",
	"description":"An extension to save the links of all all open tabs to read later",
	"version":"1.2",
	
	"browser_action":
	{
		"default_icon":"images/icon.png",
		"default_popup":"popup.html"
	},

	"permissions": 
	[
		"activeTab",
		"tabs",
		"bookmarks"
	],
	"icons": { 
	   "16": "images/16.png",
           "48": "images/48.png",
          "128": "images/128.png" }
}

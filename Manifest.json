{
	"manifest_version" : 2,
	"name":"Link Save",
	"description":"An extension to save the links of all all open tabs to read later",
	"version":"1.0",
	
	"browser_action":
	{
		"default_icon":"images/icon.png",
		"default_popup":"popup.html"
	},

	"permissions": 
	[
		"activeTab",
		"tabs"
	],

	"icons":
	{
        "128": "images/128.png"
    }
}

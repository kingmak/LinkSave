{
	"manifest_version" : 2,
	"name":"Link Save",
	"version":"1.0",

	"description":"An extension to save the links of all open tabs to read later",
	"icons":
	{
        "128": "images/128.png"
    },
	
	"browser_action":
	{
		"default_icon":"images/icon.png",
		"default_popup":"popup.html"
	},

	"author": "Muhammad Khalid & Umayah Abdennabi",
	
	"background":
	{
		"scripts":
		[
			"js/rightClick.js"
		]
    },

	"permissions": 
	[
		"contextMenus",
		"activeTab",
		"tabs"
	]
}

